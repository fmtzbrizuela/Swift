//  Genera un arreglo de interos del 0 al 100 y luego los imprime
// de acuerdo a cuatro reglas dadas.

var nums = 0...100
for num in nums {
    if num%5 == 0 { print("\(num)  Bingo!!!") }
    if num%2 == 0 { print("\(num) Par") }
    if num%2 != 0 { print("\(num) Inpar") }
    if num >= 30 && num <= 40 { print("\(num) Viva Swift!!!") }
}
